    "laravel/framework": "5.5.*",
    "laravel-rocket/foundation": "2.*",
    "laravel-rocket/generator": "2.*",
    "laravel/socialite": "^3.0",
    "facebook/graph-sdk": "^5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "friendsofphp/php-cs-fixer": "~2.2",
    "fzaninotto/faker": "~1.5",
    "laravel/laravel": "5.5.*",
    "laravel/browser-kit-testing": "^2.0"
