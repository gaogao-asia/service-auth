{
  "name": "laravel-rocket/service-auth",
  "description": "Service Authentication for Laravel Rocket",
  "type": "library",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/laravel-rocket/foundation"
    },
    {
      "type": "vcs",
      "url": "https://github.com/laravel-rocket/generator"
    }
  ],
